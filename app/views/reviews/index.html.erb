<div class="box col-md-12 col-md-offset-0 col-sm-10 col-sm-offset-1">	
	<div class="col-sm-3">
		<% 
			ratings 	= Rating.all.count
			reviews 	= Review.all.count
			data 		= { Ratings: ratings, Reviews: reviews }
		%>
		<%= render 'shared/dashboard', { title: 'Dashboard', data: data } %>
	</div>
	<div class="col-sm-9">
		<div class="title">Reviews</div>
		<div class="indexList-header row">
			<div class="col-xs-1">ID</div>
			<div class="col-xs-2">Author</div>
			<div class="col-xs-2">Lender</div>
			<div class="col-xs-3">Stars</div>
			<div class="col-xs-3">Created Date</div>
		</div>
		<%= render 'review_index_list' %>
	</div>
</div>